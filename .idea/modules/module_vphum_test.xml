<?xml version="1.0" encoding="UTF-8"?>
<project name="module_vphum_test" default="compile.module.vphum_test">
  <dirname property="module.vphum_test.basedir" file="${ant.file.module_vphum_test}"/>
  
  <property name="module.jdk.home.vphum_test" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.vphum_test" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.vphum_test" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.vphum_test" value="-encoding UTF-8 -source 1.5 -target 1.5 ${compiler.args}"/>
  
  <property name="vphum_test.output.dir" value="undefined"/>
  <property name="vphum_test.testoutput.dir" value="${module.vphum_test.basedir}/../../build/classes/test"/>
  
  <path id="vphum_test.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="vphum_test.module.production.classpath">
    <path refid="${module.jdk.classpath.vphum_test}"/>
    <pathelement location="${vphum_main.output.dir}"/>
    <path refid="library.gradle:_junit:junit:4.11.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
  </path>
  
  <path id="vphum_test.runtime.production.module.classpath">
    <path refid="vphum_main.runtime.production.module.classpath"/>
    <path refid="library.gradle:_junit:junit:4.11.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
  </path>
  
  <path id="vphum_test.module.classpath">
    <path refid="${module.jdk.classpath.vphum_test}"/>
    <pathelement location="${vphum_main.output.dir}"/>
    <path refid="library.gradle:_junit:junit:4.11.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
  </path>
  
  <path id="vphum_test.runtime.module.classpath">
    <pathelement location="${vphum_test.testoutput.dir}"/>
    <path refid="vphum_main.runtime.module.classpath"/>
    <path refid="library.gradle:_junit:junit:4.11.classpath"/>
    <path refid="library.gradle:_org.hamcrest:hamcrest-core:1.3.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.vphum_test">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.vphum_test">
    <patternset refid="excluded.from.module.vphum_test"/>
  </patternset>
  
  
  <target name="compile.module.vphum_test" depends="compile.module.vphum_test.production,compile.module.vphum_test.tests" description="Compile module vphum_test"/>
  
  <target name="compile.module.vphum_test.production" depends="register.custom.compilers,compile.module.vphum_main" description="Compile module vphum_test; production classes"/>
  
  <target name="compile.module.vphum_test.tests" depends="register.custom.compilers,compile.module.vphum_test.production" description="compile module vphum_test; test classes" unless="skip.tests"/>
  
  <target name="clean.module.vphum_test" description="cleanup module">
    <delete dir="${vphum_test.output.dir}"/>
    <delete dir="${vphum_test.testoutput.dir}"/>
  </target>
</project>